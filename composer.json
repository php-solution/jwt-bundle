{
    "name": "php-solution/jwt-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for JWT integration with lcobucci/jwt.",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Savchenko",
            "email": "dev.olsav@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "lcobucci/jwt": "^4.1",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/config": "^4.4|^5.0"
    },
    "autoload":     {
        "psr-4": { "PhpSolution\\JwtBundle\\": "src/" }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
